<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlProject.mainView.Temporar">
   <children>
      <TabPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="Acasa">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #D6EAF8;">
                     <children>
                        <ImageView fitHeight="203.0" fitWidth="596.0" layoutX="193.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@pictures/Unitbv%20Logo.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Student">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #00c3ff;">
                     <children>
                        <Button layoutX="200.0" layoutY="100.0" mnemonicParsing="false" onAction="#goAddStudentScene" prefHeight="50.0" prefWidth="200.0" text="Adauga student" textAlignment="CENTER">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="200.0" layoutY="200.0" mnemonicParsing="false" onAction="#goStudentListScene" prefHeight="50.0" prefWidth="200.0" text="Lista studenti">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <ImageView fitHeight="207.0" fitWidth="267.0" layoutX="480.0" layoutY="285.0">
                           <image>
                              <Image url="@pictures/Student.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Catalog">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #00c3ff;">
                     <children>
                        <Button layoutX="200.0" layoutY="100.0" mnemonicParsing="false" onAction="#goAddNewRegistry" prefHeight="50.0" prefWidth="200.0" text="Adauga nota">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="200.0" layoutY="200.0" mnemonicParsing="false" onAction="#goRegistryList" prefHeight="50.0" prefWidth="200.0" text="Vezi note">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <ImageView fitHeight="250.0" fitWidth="300.0" layoutX="440.0" layoutY="222.0">
                           <image>
                              <Image url="@pictures/Catalog.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Administratie">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #00c3ff;">
                     <children>
                        <ImageView fitHeight="546.0" fitWidth="674.0" layoutX="54.0" layoutY="3.0" style="-fx-opacity: 0.2;">
                           <image>
                              <Image url="@pictures/Administratie.png" />
                           </image>
                        </ImageView>
                        <Button layoutX="125.0" layoutY="50.0" mnemonicParsing="false" onAction="#goTeachersList" prefHeight="50.0" prefWidth="200.0" text="Vezi profesori">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="475.0" layoutY="50.0" mnemonicParsing="false" onAction="#goAddNewTeacher" prefHeight="50.0" prefWidth="200.0" text="Adauga profesor">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="475.0" layoutY="150.0" mnemonicParsing="false" onAction="#goAddNewSubject" prefHeight="50.0" prefWidth="200.0" text="Adauga materie">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="125.0" layoutY="150.0" mnemonicParsing="false" onAction="#goListSubjects" prefHeight="50.0" prefWidth="200.0" text="Vezi materii">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="125.0" layoutY="250.0" mnemonicParsing="false" onAction="#goListProfile" prefHeight="50.0" prefWidth="200.0" text="Vezi profile">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="475.0" layoutY="250.0" mnemonicParsing="false" onAction="#goAddNewProfile" prefHeight="50.0" prefWidth="200.0" text="Adauga profil">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="125.0" layoutY="350.0" mnemonicParsing="false" onAction="#goGroupList" prefHeight="50.0" prefWidth="200.0" text="Vezi grupe">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="475.0" layoutY="350.0" mnemonicParsing="false" onAction="#goAddNewGroup" prefHeight="50.0" prefWidth="200.0" text="Adauga grupe">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="125.0" layoutY="450.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Burse">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                        <Button layoutX="475.0" layoutY="450.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Conferinte">
                           <font>
                              <Font name="Arial" size="18.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
